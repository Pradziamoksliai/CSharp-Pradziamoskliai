<Window x:Class="irasymasIsTextBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:irasymasIsTextBox"
        mc:Ignorable="d"
        Title="Numerio įvedimas" Height="143.182" Width="440.431" ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="32,47,0,0" TextWrapping="Wrap" Text="Numeris:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="21" Margin="84,47,0,0" TextWrapping="Wrap" Text="(pvz.: 860000000)" VerticalAlignment="Top" Width="196" Foreground="#FFAEA4A4" TextChanged="textBox_TextChanged"/>
        <Button x:Name="button" Content="Įrašyti" HorizontalAlignment="Left" Margin="303,27,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="Nr. sąrašas" HorizontalAlignment="Left" Margin="303,63,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>

    </Grid>
</Window>
